{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "upload": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/upload",
      "sourceRoot": "projects/upload/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/upload",
            "index": "projects/upload/src/index.html",
            "main": "projects/upload/src/main.ts",
            "polyfills": "projects/upload/src/polyfills.ts",
            "tsConfig": "projects/upload/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/upload/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "projects/core/src/lib/assets/images",
                "output": "assets/images"
              },
              "projects/upload/src/upload-config.js"
            ],
            "styles": ["projects/core/src/lib/assets/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/upload/src/environments/environment.ts",
                  "with": "projects/upload/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "fr": {
              "aot": true,
              "outputPath": "dist/upload/fr/",
              "i18nFile": "projects/core/src/lib/locale/messages.fr.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "fr",
              "i18nMissingTranslation": "error",
              "baseHref": "/upload/fr/"
            },
            "en": {
              "aot": true,
              "outputPath": "dist/upload/en/",
              "i18nFile": "projects/core/src/lib/locale/messages.en.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en",
              "i18nMissingTranslation": "error",
              "baseHref": "/upload/en/"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "upload:build",
            "port": 4111
          },
          "configurations": {
            "production": {
              "browserTarget": "upload:build:production"
            },
            "fr": {
              "browserTarget": "upload:build:fr"
            },
            "en": {
              "browserTarget": "upload:build:en"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "upload:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/upload/src/test.ts",
            "polyfills": "projects/upload/src/polyfills.ts",
            "tsConfig": "projects/upload/tsconfig.spec.json",
            "karmaConfig": "projects/upload/karma.conf.js",
            "assets": [
              "projects/upload/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "projects/core/src/lib/assets/images",
                "output": "assets/images"
              },
              "projects/upload/src/upload-config.js"
            ],
            "styles": ["projects/core/src/lib/assets/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/upload/tsconfig.app.json",
              "projects/upload/tsconfig.spec.json",
              "projects/upload/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/upload/e2e/protractor.conf.js",
            "devServerTarget": "upload:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "upload:serve:production"
            }
          }
        }
      }
    },
    "download": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/download",
      "sourceRoot": "projects/download/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/download",
            "index": "projects/download/src/index.html",
            "main": "projects/download/src/main.ts",
            "polyfills": "projects/download/src/polyfills.ts",
            "tsConfig": "projects/download/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/download/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "projects/core/src/lib/assets/images",
                "output": "assets/images"
              },
              "projects/download/src/download-config.js"
            ],
            "styles": ["projects/core/src/lib/assets/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/download/src/environments/environment.ts",
                  "with": "projects/download/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "download:build",
            "port": 4222
          },
          "configurations": {
            "production": {
              "browserTarget": "download:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "download:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/download/src/test.ts",
            "polyfills": "projects/download/src/polyfills.ts",
            "tsConfig": "projects/download/tsconfig.spec.json",
            "karmaConfig": "projects/download/karma.conf.js",
            "assets": [
              "projects/download/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "projects/core/src/lib/assets/images",
                "output": "assets/images"
              },
              "projects/download/src/download-config.js"
            ],
            "styles": ["projects/core/src/lib/assets/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/download/tsconfig.app.json",
              "projects/download/tsconfig.spec.json",
              "projects/download/e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/download/e2e/protractor.conf.js",
            "devServerTarget": "download:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "download:serve:production"
            }
          }
        }
      }
    },
    "core": {
      "projectType": "library",
      "root": "projects/core",
      "sourceRoot": "projects/core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/core/tsconfig.lib.json",
            "project": "projects/core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core/src/test.ts",
            "tsConfig": "projects/core/tsconfig.spec.json",
            "karmaConfig": "projects/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/core/tsconfig.lib.json", "projects/core/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "upload"
}
